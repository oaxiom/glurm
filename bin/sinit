#!/usr/bin/env python3

import sys
import os
import logging

from glurm import db

if __name__ == '__main__':
    log = logging.getLogger('glurm')
    db = db(log=log, init=True) # Can init;

    # We are debugging, so reset on each sprocess for now:
    os.remove(os.path.expanduser('~/.glurm/database.db'))
    db.setup()
